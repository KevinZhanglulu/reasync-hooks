  "main": "./lib/reasync-hooks.js",
  "module": "./es/reasync-hooks.js",
  "unpkg": "./dist/reasync-hooks.js",
  "types": "./lib/index.d.ts",
  "keywords": [
    "react",
    "hooks",
    "async",
    "redux"
  ],
  "author": "Kevin Zhang <kevinzhanglulu@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
